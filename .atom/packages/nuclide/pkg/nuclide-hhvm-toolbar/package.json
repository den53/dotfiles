{
  "name": "nuclide-hhvm-toolbar",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0.1.0": "consumeToolBar"
      }
    }
  },
  "providedServices": {
    "nuclide-distraction-free-mode": {
      "versions": {
        "0.0.0": "getDistractionFreeModeProvider"
      }
    }
  },
  "description": "Provides a toolbar in Nuclide to build projects with HHVM"
}
